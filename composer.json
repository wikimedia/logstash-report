{
    "type": "wikimedia/logstash-report",
    "license": "GPL-2.0+",
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/console": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.3",
        "symfony/property-access": "^3.3",
        "symfony/serializer": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "mediawiki/mediawiki-codesniffer": "^0.12.0",
        "symfony/dotenv": "^3.3",
        "symfony/var-dumper": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BS6XMQXB9R0283SRQ2ENNT89",
            "allow-contrib": false
        }
    }
}
